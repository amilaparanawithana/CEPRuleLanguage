<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<query>
    <select>
        <attributes>
            <attribute as="attr1o">atr1</attribute>
            <attribute as="attr2o" type="DEFAULT">atr2</attribute>
        </attributes>
    </select>

    <from>
        <streams>
            <stream as="st1">
                <name>st1</name>
                <window func="time">
                    <parameters>
                        <parameter>para1</parameter>
                        <parameter>para2</parameter>
                    </parameters>
                </window>
                <filter>stream condision</filter>
            </stream>

            <stream as="st2">
                <name>st2</name>
                <window func="length">
                    <parameters>
                        <parameter>para1</parameter>
                    </parameters>
                </window>
                <filter>stream2condision</filter>
            </stream>
        </streams>
    </from>
    <where>whereCondision</where>
</query>




